{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": ["import", "node", "promise", "standard"],
    "env": {
      "browser": true,
      "node": true,
      "es6":true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": 0
    }
  }